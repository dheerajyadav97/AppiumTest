<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Appium Test Suite" verbose="1" parallel="tests" thread-count="5">
    <test name="launch app">
        <classes>
            <class name="appium.test.LaunchApp"/>
        </classes>
    </test>
    <test name="App test">
        <classes>
            <class name="appium.test.AppTest"/>
        </classes>
    </test>
</suite>